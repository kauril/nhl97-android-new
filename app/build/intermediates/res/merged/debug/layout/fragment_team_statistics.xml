<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.iosdev.nhl97.TeamStatisticsFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_height="wrap_content"
        android:id="@+id/mainTeamStatisticsLinearLayout">

    <LinearLayout
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="0dp"
        android:layout_marginLeft="85dp">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:id="@+id/nameTV"
            android:paddingBottom="25dp"
            android:paddingTop="25dp"
            android:textSize="20sp"/>
        <NumberPicker
            android:id="@+id/tp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/teamTV"
            android:textAlignment="center"
            android:paddingTop="15dp"
            android:textSize="20sp"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        >
        <LinearLayout
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="50dp"
            android:id="@+id/teamStatisticsFirstColumn"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsGamesPlayed"
                android:text="@string/games_played"
                android:paddingBottom="5dp"
                android:paddingTop="25dp" />
            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamGamesPlayed"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsWins"
                android:text="@string/wins"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />
            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamWins"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsloses"
                android:text="@string/loses"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />
            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamLoses"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsWinningPercentage"
                android:text="@string/winning_percentage"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />
            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamWinningPercentage"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsGoals_for"
                android:text="@string/gf"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamGoalsFor"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsGoals_against"
                android:text="@string/ga"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamGoalsAgainst"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsGoalsForPerGame"
                android:text="@string/goals_for_per_game"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamGoalsForPerGame"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsAgainstForPerGame"
                android:text="@string/goals_against_per_game"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamGoalsAgainstPerGame"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsGoalPercentage"
                android:text="@string/gp"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamGoalPersentage"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsSavePercentage"
                android:text="@string/sp"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamSavePercentage"/>


        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        >
        <LinearLayout
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="50dp"
            android:id="@+id/teamStatisticsSecondColumn"
            >



            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsShots_for"
                android:text="@string/sf"
                android:paddingBottom="5dp"
                android:paddingTop="25dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamShotsFor"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsShots_against"
                android:text="@string/sa"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamShotsAgainst"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsShotsForPerGame"
                android:text="@string/shots_for_per_game"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamShotsForPerGame"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsShotsAgainstPerGame"
                android:text="@string/shots_against_per_game"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamShotsAgainstPerGame"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsOvertimes"
                android:text="@string/overtimes"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamOvertimes"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsOvertimeWins"
                android:text="@string/overtime_wins"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamOvertimeWins"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsOvertimeLoses"
                android:text="@string/overtime_loses"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamOvertimeLoses"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsShootouts"
                android:text="@string/shootouts"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamShootouts"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsShootoutWins"
                android:text="@string/shootout_wins"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamShootoutWins"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:id="@+id/teamStatisticsShootoutLoses"
                android:text="@string/shootout_loses"
                android:paddingBottom="5dp"
                android:paddingTop="5dp" />

            <TextView
                android:layout_width="match_parent"
                android:textAlignment="center"
                android:layout_height="wrap_content"
                android:id="@+id/teamShootoutLoses"/>


        </LinearLayout>

    </LinearLayout>
        <LinearLayout
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="50dp"
            android:layout_marginRight="50dp"
            >
            <LinearLayout
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="0dp"
                android:layout_marginRight="50dp"
                android:id="@+id/teamStatisticsThirdColumn"
                >




                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:id="@+id/teamStatisticsHomeWins"
                    android:text="@string/home_wins"
                    android:paddingBottom="5dp"
                    android:paddingTop="25dp" />

                <TextView
                    android:layout_width="match_parent"
                    android:textAlignment="center"
                    android:layout_height="wrap_content"
                    android:id="@+id/teamHomeWins"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:id="@+id/teamStatisticsHomeLoses"
                    android:text="@string/home_loses"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" />

                <TextView
                    android:layout_width="match_parent"
                    android:textAlignment="center"
                    android:layout_height="wrap_content"
                    android:id="@+id/teamHomeLoses"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:id="@+id/teamStatisticsGuestWins"
                    android:text="@string/guest_wins"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" />

                <TextView
                    android:layout_width="match_parent"
                    android:textAlignment="center"
                    android:layout_height="wrap_content"
                    android:id="@+id/teamGuestWins"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:id="@+id/teamStatisticsGuestLoses"
                    android:text="@string/guest_loses"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" />

                <TextView
                    android:layout_width="match_parent"
                    android:textAlignment="center"
                    android:layout_height="wrap_content"
                    android:id="@+id/teamGuestLoses"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:id="@+id/teamStatisticsKspPercentage"
                    android:text="ksp winning persentage"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" />
                <TextView
                    android:layout_width="match_parent"
                    android:textAlignment="center"
                    android:layout_height="wrap_content"
                    android:id="@+id/kpsPercentage"/>

            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

</FrameLayout>
